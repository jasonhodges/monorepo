{
  "name": "monorepo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel start:*",
    "start:IU": "ng serve IndianaUniversity --port 4201 -o",
    "start:MU": "ng serve MarylandUniversity --port 4202 -o",
    "start:MSU": "ng serve MichiganStateUniversity --port 4203 -o",
    "start:OSU": "ng serve OhioStateUniversity --port 4204 -o",
    "start:PSU": "ng serve PennStateUniversity --port 4205 -o",
    "start:RUTG": "ng serve RutgersUniversity --port 4206 -o",
    "start:MICH": "ng serve UniversityOfMichigan --port 4207 -o",
    "start:ILL": "ng serve UniversityOfIllinois --port 4301 -o",
    "build": "npm-run-all -s build:lib-* build:*",
    "build-apps": "npm-run-all --parallel build:*",
    "build:IU": "ng build IndianaUniversity --progress=false",
    "build:MU": "ng build MarylandUniversity --progress=false",
    "build:MSU": "ng build MichiganStateUniversity --progress=false",
    "build:OSU": "ng build OhioStateUniversity --progress=false",
    "build:PSU": "ng build PennStateUniversity --progress=false",
    "build:RUTG": "ng build RutgersUniversity --progress=false",
    "build:MICH": "ng build UniversityOfMichigan --progress=false",
    "build:ILL": "ng build UniversityOfIllinois --progress=false",
    "build-libs": "npm-run-all build-lib:lib-*",
    "build-lib:lib-header": "ng build lib-header",
    "build-lib:lib-player-card": "ng build lib-player-card",
    "test": "npm-run-all --parallel test:* -c",
    "test:IU": "ng test IndianaUniversity",
    "test:MU": "ng test MarylandUniversity",
    "test:MSU": "ng test MichiganStateUniversity",
    "test:OSU": "ng test OhioStateUniversity",
    "test:PSU": "ng test PennStateUniversity",
    "test:RUTG": "ng test RutgersUniversity",
    "test:MICH": "ng test UniversityOfMichigan",
    "test:ILL": "ng test UniversityOfIllinois",
    "watch-libs": "npm-run-all --parallel watch:lib*",
    "watch:lib-header": "ng build lib-header --watch",
    "watch:lib-player-card": "ng build lib-player-card --watch",
    "deploy-sites": "npm-run-all -p deploy:*",
    "deploy:IU": "NETLIFY_TOKEN=$monorepo_01_netlify ng run IndianaUniversity:deploy",
    "deploy:MICH": "NETLIFY_TOKEN=$monorepo_01_netlify ng run UniversityOfMichigan:deploy",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier:write": "prettier --config ./.prettierrc --list-different \"projects/*/{src/app,src/environments,src/assets}/**/*{.ts,.js,.json,.css,.scss}\" --write",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.1.1",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@netlify-builder/deploy": "^2.0.2",
    "@nrwl/angular": "^8.5.1",
    "@nrwl/workspace": "^8.5.1",
    "hammerjs": "^2.0.8",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@nrwl/jest": "8.5.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.1.0",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "jest-preset-angular": "7.0.0"
  }
}
